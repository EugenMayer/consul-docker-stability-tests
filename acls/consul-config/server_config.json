{
  "datacenter": "stable",
  "data_dir": "/consul/data",
  "ui": true,
  "dns_config": {
    "allow_stale": false
  },
  "addresses": {
    "http": "127.0.0.1",
    "https": "0.0.0.0"
  },
  "ports": {
    "http": 8500,
    "https": 8501
  },
  "node_name": "myconsulserver",
  "client_addr": "0.0.0.0",
  "server": true,
  "bootstrap_expect": 1,
  "check_update_interval": "0s",
  "disable_update_check": true
}